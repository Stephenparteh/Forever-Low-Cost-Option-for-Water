<%- include ('layouts/dashboard'); %>

    <!-- View Logs Section -->
    <div id="view-log" class="col-md-9 ml-sm-auto col-lg-10 px-4 content"
        style="position: absolute; top: 20px; right: 20px;">
        <div class="card-header">
            <h4>View Logs</h4>
        </div>
        <div class="card-body">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Log Type</th>
                        <th>Description</th>
                        <th>Date</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Example Row -->
                    <tr>
                        <td>1</td>
                        <td>System</td>
                        <td>Water source Central Well added</td>
                        <td>2024-08-15</td>
                        <td>Success</td>
                    </tr>
                    <!-- More rows as needed -->
                </tbody>
            </table>
        </div>
    </div>