<%- include ('layouts/dashboard'); %>

    <!-- Manage Users Section -->
    <div id="manage-user" class="col-md-9 ml-sm-auto col-lg-10 px-4 content"
        style="position: absolute; top: 20px; right: 20px;">
        <div class="card-header">
            <h4>Manage Users</h4>
        </div>
        <div class="card-body">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Username</th>
                        <th>Email</th>
                        <th>Phone Number</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Example Row -->
                    <% totalUsers.forEach(user=> { %>
                        <tr>
                            <td>
                                <%= count++ %>
                            </td>
                            <td>
                                <%= user.first_name %>
                            </td>
                            <td>
                                <%= user.last_name%>
                            </td>
                            <td>
                                <%= user.username %>
                            </td>
                            <td>
                                <%= user.email %>
                            </td>
                            <td>
                                <%= user.phone_number %>
                            </td>
                            <td>
                                <button class="btn btn-sm btn-warning">Edit</button>
                                <button class="btn btn-sm btn-danger">Delete</button>
                            </td>
                        </tr>
                        <% }) %>

                            <!-- More rows as needed -->
                </tbody>
            </table>
            <a class="btn btn-primary" href="/add-user">Add New User</a>
        </div>
    </div>